    "uuid": "3.4.0",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "2.0.3",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint '**/*.js'",
    "lint-fix": "eslint --fix '**/*.js'"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "resolver": "jest-pnp-resolver",
    "coveragePathIgnorePatterns": [
      "registerServiceWorker.js",
      "sagas.js",
      "mocks.js",
      "src/index.js",
      "src/components/AnalysisOutputTree",
      "src/components/AsyncSelect",
      "src/components/DataFileTree",
      "src/components/ProtocolPlanTable",
      "src/components/TaskList",
      "src/containers/Pages/DataDirectory/index.js",
      "src/containers/Pages/DataDirectory/ExpandedRow.js",
      "src/containers/Sections/Upload",
      "src/store/modules/datafiles/reducer.js",
      "src/polyfills/localStorage.js",
      "src/routes/index.js",
      "src/store/index.js",
      "src/store/reducers.js",
      "src/store/sagas.js",
      "src/store/modules/(.*)/index.js",
      "src/utils/auth-wrappers.js"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "jest-localstorage-mock",
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "jest-localstorage-mock": "^2.3.0",
    "pre-commit": "^1.2.2",
    "redux-saga-test-plan": "^3.7.0",
    "sinon": "^9.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pre-commit": {
    "colors": true,
    "silent": false,
    "run": [
      "lint"
    ]
  }
}
